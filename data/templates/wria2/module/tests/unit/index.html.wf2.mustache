<!doctype html>
<html>
<head>
    <title>{{className}}</title>
    <script src="../../../../build/yui/yui-min.js"> </script>
    <script src="js/tests.js"></script>
    <meta charset="utf-8" />
</head>
<body class="yui3-skin-sam" style="margin:8px;">
  <div class="wf2-skin-nx wf2-skin-nxt example">
  </div>
  <div id="logger"></div>
<script>
YUI({
  combine: false,
  skin: {
    overrides: {
      'test-console': ['sam'],
      'console': ['sam'],
      'console-filters': ['sam']
    }
  },
  coverage: ['{{moduleName}}'],
  filter: (window.location.search.match(/[?&]filter=([^&]+)/) || [])[1] || 'raw',
  allowRollup: false,
  modules: {
    '{{moduleName}}-tests': {
        fullpath: './js/tests.js',
        requires: ['{{moduleName}}', 'test']
    }
  }
}).use('{{moduleName}}-tests', 'test-console', 'test', 'module-tests', function(Y) {
  (new Y.Test.Console({filters: {pass:true}})).render('#logger');
  Y.Test.Runner.setName('{{moduleName}}');
  Y.Test.Runner.run();
});
</script>
</body>
</html>
